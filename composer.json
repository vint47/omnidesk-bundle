{
  "name": "vragovr/omnidesk-bundle",
  "description": "Omnidesk API Bundle",
  "version": "0.0.1",
  "type": "library",
  "keywords": [
    "symfony",
    "bundle",
    "api",
    "omnidesk"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Roman Vragov",
      "email": "vragovR@yandex.ru"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "symfony/dependency-injection": "^3.3",
    "symfony/config": "^3.3",
    "symfony/http-foundation": "^3.3",
    "symfony/http-kernel": "^3.3",
    "doctrine/collections": ">=1.4",
    "guzzlehttp/guzzle": "^6.2"
  },
  "require-dev": {
    "phpunit/phpunit": "6.0.*"
  },
  "autoload": {
    "psr-4": {
      "OmnideskBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OmnideskBundle\\": "tests/"
    }
  }
}
